{
  "expo": {
    "name": "MyTrainPro",
    "slug": "mytrainpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "description": "AI-powered fitness coaching with evidence-based periodization, nutrition tracking, and personalized workout plans for serious fitness enthusiasts.",
    "keywords": ["fitness", "AI", "workout", "nutrition", "bodybuilding", "training", "health"],
    "privacy": "public",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.trainpro.app",
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "MyTrainPro needs camera access to scan barcodes for food logging.",
        "NSPhotoLibraryUsageDescription": "MyTrainPro needs photo library access to upload progress photos.",
        "UIBackgroundModes": ["fetch"],
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.trainpro.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b0fa7770-552d-4612-8f93-3b8944c63ef3"
      }
    }
  }
}